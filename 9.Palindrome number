3.   Palindrome Number

class Solution {
    public boolean isPalindrome(int x) {
     int rem,sum=0,temp;
     temp=x;   //store x in temp
     while(x>0){
        rem=x%10; 
        sum=(sum*10)+rem;
        x=x/10;
     }
     if(sum==temp)
     return true;
     else;
     return false;

   }    
}




